{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.3.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "git-deploy": "cp -r ./build/* ../dampion.github.io/react/F2E/todolist/",
    "dev": "npm-run-all --parallel watch",
    "css": "node-sass --ext .scss src --output ./src/assets/css",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm run css\""
  }
}
